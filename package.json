    "express-handlebars": "^5.3.0",
    "socket.io": "^4.0.1",
    "socket.io-client": "^4.0.1"
  },
  "preferGlobal": true,
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^7.25.0",
    "eslint-config-google": "^0.14.0",
    "mocha": "^8.3.2",
    "nyc": "^15.1.0"
  }
}
