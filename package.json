{
  "name": "buildoff",
  "version": "1.0.0",
  "description": "buildoff",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BananoCoin/buildoff.git"
  },
  "scripts": {
    "new-config": "node scripts/new-config.js",
    "start": "node -max-old-space-size=1700 scripts/index.js",
